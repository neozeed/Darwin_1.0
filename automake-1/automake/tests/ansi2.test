#! /bin/sh

# Test to make sure ansi2knr built at the right time.

. $srcdir/defs || exit 1

cat > Makefile.am << 'END'
AUTOMAKE_OPTIONS = ansi2knr
bin_PROGRAMS = joe
END

cat >> configure.in << 'END'
AC_PROG_CC
AM_C_PROTOTYPES
END

: > ansi2knr.c
: > ansi2knr.1

$AUTOMAKE || exit 1

grep '^all[-a-z]*:.*ANSI2KNR' Makefile.in
