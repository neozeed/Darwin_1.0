#! /bin/sh

# Test for a bug reported by Akim Demaille.
# LIBOBJS specified in the "wrong order" aren't seen.

. $srcdir/defs || exit 1

cat > configure.in << 'END'
PACKAGE=nonesuch
VERSION=nonesuch
AC_PROG_CC
AC_ARG_PROGRAM
AC_PROG_INSTALL
AC_PROG_RANLIB
LIBOBJS="fsusage.o mountlist.o $LIBOBJS"
AC_OUTPUT(Makefile)
END

cat > Makefile.am << 'END'
noinst_LIBRARIES = libtu.a
libtu_a_SOURCES =
libtu_a_LIBADD = @LIBOBJS@
END

: > fsusage.c
: > mountlist.c

$AUTOMAKE
